{

  "PhoneParser": {
    "AcceptanceRules": {
      "^((?<id>[0-9]*),)?(?<phone>27[0-9]{9})$": "Accepted"
    },
    "CorrectionRules": {
      "^((?<id>[0-9]*),)?(?<phone>[0-9]{9})$": "Missing Prefix"
    }
},

  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
